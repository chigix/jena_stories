    <owl:onProperty rdf:resource="http://chigix.com/else#simpleProperty"/>
  </owl:Restriction>
  <owl:Restriction>
    <owl:allValuesFrom rdf:resource="http://chigix.com/else#NewClass"/>
    <owl:onProperty rdf:resource="http://chigix.com/else#restrictedProperty"/>
  </owl:Restriction>
