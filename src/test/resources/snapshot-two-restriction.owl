<rdf:RDF
    xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
    xmlns:owl="http://www.w3.org/2002/07/owl#"
    xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
    xmlns:xsd="http://www.w3.org/2001/XMLSchema#">
  <owl:Class rdf:about="http://chigix.com/else#NewClass"/>
  <owl:ObjectProperty rdf:about="http://chigix.com/else#restrictedProperty"/>
  <rdf:Property rdf:about="http://chigix.com/else#simpleProperty"/>
